@tailwind base;
@tailwind components;
@tailwind utilities;

* {
    @apply antialiased;
}

body {
    @apply bg-white;
    @apply dark:bg-neutral-900;
    @apply text-black;
    @apply dark:text-white;
    @apply font-normal;
    font-family: 'Rubik', sans-serif;
}

h1, h2, h3, h4, h5, h6 {
    @apply font-medium;
    font-family: 'Rubik', sans-serif;
}